<%= form_for @resume_entry do |f| %>
	<div class="field">
		<%= f.label :start_date %><br>
		<%= f.text_field :start_date %>
	</div>
	<div class="field">
		<%= f.label :end_date %><br>
		<%= f.text_field :end_date %>
	</div>
	<div class="field">
		<%= f.label :description %><br>
		<%= f.text_area :description %>
	</div>
	<div class="actions">
		<%= f.submit "Submit" %>
	</div>
<% end %>